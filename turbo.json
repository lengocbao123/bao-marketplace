{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "NEXT_PUBLIC_GOOGLE_TAG_MANAGER_ID",
        "OPENAPI_BASE_URL",
        "ENCRYPTION_SECRET",
        "NEXTAUTH_SECRET",
        "BUILDER_URL"
      ]
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
